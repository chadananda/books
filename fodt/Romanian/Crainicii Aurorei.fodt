    </draw:frame><text:span text:style-name="T39"> CAPITOLUL I</text:span></text:p>
   <text:p text:style-name="P66"><draw:frame text:anchor-type="char" draw:z-index="1" draw:style-name="gr1" draw:text-style-name="P102" svg:width="8.526cm" svg:height="0.221cm" svg:x="0cm" svg:y="27.4cm">
