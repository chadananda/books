    <text:p text:style-name="Standard"><text:span text:style-name="T13">A</text:span><text:span text:style-name="T14">ron,134</text:span></text:p>
    <text:p text:style-name="Standard"><text:span text:style-name="T14">&apos;Abdu&apos;l-Bahá, 50n</text:span></text:p>
    <text:p text:style-name="P16">Acţiuni; vezi Fapte <text:tab/></text:p>
