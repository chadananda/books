   <text:p text:style-name="P2"><text:span text:style-name="T1">Cele Patru V</text:span><text:span text:style-name="T1">ă</text:span><text:span text:style-name="T1">i</text:span></text:p>
   <text:p text:style-name="italic"><text:span text:style-name="T3"><text:s/></text:span><text:span text:style-name="T1">El este Cel Puternic, Cel Preaiubit</text:span></text:p>
   <text:p text:style-name="citat1"><text:span text:style-name="T1">O, lumin</text:span><text:span text:style-name="T1">ă</text:span><text:span text:style-name="T1"> a adev</text:span><text:span text:style-name="T1">ă</text:span><text:span text:style-name="T1">rului, </text:span><text:span text:style-name="T4">H</text:span><text:span text:style-name="T18">.</text:span><text:span text:style-name="T1">isám-i-Dín, cel m</text:span><text:span text:style-name="T1">ă</text:span><text:span text:style-name="T1">rinimos,</text:span></text:p>
   <text:p text:style-name="citat2"><text:span text:style-name="T1">Niciodat</text:span><text:span text:style-name="T1">ă</text:span><text:span text:style-name="T1"> lumea n-a dat na</text:span><text:span text:style-name="T22">ş</text:span><text:span text:style-name="T1">tere unui prin</text:span><text:span text:style-name="T22">ţ</text:span><text:span text:style-name="T1"> care s</text:span><text:span text:style-name="T1">ă</text:span><text:span text:style-name="T1"> semene cu </text:span><text:span text:style-name="T23">T</text:span><text:span text:style-name="T1">ine!</text:span><text:span text:style-name="Footnote_20_Characters"><text:span text:style-name="T1"><text:note text:id="ftn1" text:note-class="footnote"><text:note-citation>1</text:note-citation><text:note-body>
        <text:p text:style-name="Standard"><text:span text:style-name="Footnote_20_Characters"><text:span text:style-name="T3"/></text:span></text:p>
        <text:p text:style-name="P4"><text:span text:style-name="T1"><text:tab/> </text:span><text:span text:style-name="T5">Ma</text:span><text:span text:style-name="T6">th</text:span><text:span text:style-name="T5">návi</text:span><text:span text:style-name="T1"> a lui Rúmí.</text:span></text:p></text:note-body></text:note></text:span></text:span></text:p>
   <text:p text:style-name="Standard"><text:span text:style-name="T1">M</text:span><text:span text:style-name="T1">ă</text:span><text:span text:style-name="T1"> întreb de ce leg</text:span><text:span text:style-name="T1">ă</text:span><text:span text:style-name="T1">turile de iubire au fost rupte într-un mod atât de nea</text:span><text:span text:style-name="T22">ş</text:span><text:span text:style-name="T1">teptat </text:span><text:span text:style-name="T22">ş</text:span><text:span text:style-name="T1">i statornicul leg</text:span><text:span text:style-name="T1">ă</text:span><text:span text:style-name="T1">mânt de prietenie s-a frânt. Oare, Doamne fere</text:span><text:span text:style-name="T22">ş</text:span><text:span text:style-name="T1">te, devotamentul meu s-a mic</text:span><text:span text:style-name="T22">ş</text:span><text:span text:style-name="T1">orat sau dragostea mea nu s-a dovedit destul de puternic</text:span><text:span text:style-name="T1">ă</text:span><text:span text:style-name="T1">, astfel încât tu M-ai uitat </text:span><text:span text:style-name="T22">ş</text:span><text:span text:style-name="T1">i M-ai </text:span><text:span text:style-name="T22">ş</text:span><text:span text:style-name="T1">ters din amintirea ta? </text:span></text:p>
   <text:p text:style-name="citat1"><text:span text:style-name="T3">„</text:span><text:span text:style-name="T1">Cu ce-am gre</text:span><text:span text:style-name="T22">ş</text:span><text:span text:style-name="T1">it Eu ca s</text:span><text:span text:style-name="T1">ă</text:span><text:span text:style-name="T1"> pierd bun</text:span><text:span text:style-name="T1">ă</text:span><text:span text:style-name="T1">voin</text:span><text:span text:style-name="T22">ţ</text:span><text:span text:style-name="T1">a ta? </text:span></text:p>
   <text:p text:style-name="citat2"><text:span text:style-name="T1">Este oare pentru c</text:span><text:span text:style-name="T1">ă</text:span><text:span text:style-name="T1"> rangul Nostru e mic, iar al t</text:span><text:span text:style-name="T1">ă</text:span><text:span text:style-name="T1">u înalt?”</text:span><text:span text:style-name="Footnote_20_Characters"><text:span text:style-name="T1"><text:note text:id="ftn2" text:note-class="footnote"><text:note-citation>2</text:note-citation><text:note-body>
        <text:p text:style-name="Footnote"><text:span text:style-name="T1"><text:s/>Sa‘dí, Mu</text:span><text:span text:style-name="T7"> s</text:span><text:span text:style-name="T24">.</text:span><text:span text:style-name="T1">li</text:span><text:span text:style-name="T9"> h</text:span><text:span text:style-name="T25">.</text:span><text:span text:style-name="T1">u’d-Dín din </text:span><text:span text:style-name="T23">S</text:span><text:span text:style-name="T1">iraz (ca. 1184-1291), autor celebru al </text:span><text:span text:style-name="T5">Gulistánului</text:span><text:span text:style-name="T1"> </text:span><text:span text:style-name="T23">s</text:span><text:span text:style-name="T1">i al altor lucr</text:span><text:span text:style-name="T1">ă</text:span><text:span text:style-name="T1">ri poetice.</text:span></text:p></text:note-body></text:note></text:span></text:span></text:p>
